model Token {
    id         String   @id @default(cuid())
    createdAt  DateTime     @default(now())
    token      String       @db.VarChar(127)
    type       Boolean      @default(true)
    link       String
    ip_address String?
    submitter  String?
}

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
